@model IEnumerable<DomainLibrary.Models.ApplicationUser>

@{
    ViewBag.Title = "Sökresultat";
}

<h2>Sökresultat</h2>

<table class="table">
    <tr>
        <th>
            Förnamn
        </th>
        <th>
            Efternamn
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
     
        <td>
            @Html.ActionLink("Profil", "Index", new { id=item.Id }) 
        </td>
    </tr>
}

</table>
